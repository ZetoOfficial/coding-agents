# GitHub Configuration https://github.com/settings/tokens
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_REPOSITORY=username/repository

# LLM Provider Configuration
# Choose one: "openai" or "yandex"
LLM_PROVIDER=openai

# OpenAI Configuration (if using OpenAI)
OPENAI_API_KEY=sk-your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# YandexGPT Configuration (if using Yandex)
YANDEX_API_KEY=your_yandex_api_key_here
YANDEX_MODEL=yandexgpt-latest
YANDEX_FOLDER_ID=your_yandex_folder_id

# Agent Configuration
MAX_ITERATIONS=5
AGENT_TIMEOUT_MINUTES=30
DEFAULT_BRANCH=master
WORK_BRANCH_PREFIX=agent/issue-

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Security Configuration
MIN_COVERAGE_PERCENT=70
ENABLE_SECURITY_CHECKS=true

# Optional: Rate Limiting
MAX_LLM_REQUESTS_PER_MINUTE=10
MAX_GITHUB_REQUESTS_PER_HOUR=5000
